loadstring([[
function ClearFog()
    state = (state) == true 
    while state do
    game: service"RunService".RenderStepped:wait()
    game.Lighting.FogEnd = 9999
    end
end
]])();
----------------//funtion//-------------------
loadstring([[
function SpookMode()
game.Lighting.Spook.Value = state
end
]])();
----------------//funtion//-------------------
loadstring([[
function RemoveLava()
    for i,v in pairs(game.Workspace.Region_Volcano:GetDescendants()) do
        if v.Name == "TouchInterest" then
          v:Destroy()
        elseif v.Name == "Lava" then
          for i,c in pairs(v:GetChildren()) do
            if c:IsA("Part") then
              if state == true then
                c.Transparency = 1
              else    
                c.Transparency = 0
              end
            end
          end
        end
      end
end
]])();
----------------//funtion//-------------------
loadstring([[
function Boulders()
    local destroy = true
    local boulders = workspace.Region_Volcano:FindFirstChild("PartSpawner")
    while destroy == true do
    for i, v in pairs(boulders:GetChildren()) do
     v:Destroy()
            end
            wait(2)
        end 
end
]])();
