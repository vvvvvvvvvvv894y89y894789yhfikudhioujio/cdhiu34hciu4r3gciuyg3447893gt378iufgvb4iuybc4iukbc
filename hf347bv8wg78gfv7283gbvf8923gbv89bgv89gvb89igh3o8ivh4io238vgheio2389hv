if value == true then
for i,v in pairs(game.Workspace.Region_Snow:GetChildren()) do
if v.Name == "Snow" then
v.Transparency = 1
end
end
else
for i,v in pairs(game.Workspace.Region_Snow:GetChildren()) do
if v.Name == "Snow" then
v.Transparency = 0
end
end
end
