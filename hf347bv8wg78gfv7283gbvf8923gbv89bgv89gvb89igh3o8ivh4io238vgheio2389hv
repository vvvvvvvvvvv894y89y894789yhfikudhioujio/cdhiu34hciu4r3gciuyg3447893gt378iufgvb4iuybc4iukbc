loadstring([[
function leakeditems(value)
    if value == true then
        game.ReplicatedStorage.Purchasables:Clone().Parent = game.Workspace.PlayerModels
        end
        if value == false then 
        game.ReplicatedStorage.Purchasables:Clone().Parent = game.Workspace.PlayerModels:Destroy()
        end
        end
    ]])();

